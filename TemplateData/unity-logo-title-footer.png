    const AD_ELEMENT_TO_KEY = {};
    Object.keys(AD_CONFIGS).forEach(function (key) {
      var cfg = AD_CONFIGS[key];
      if (cfg && cfg.elementId) {
        AD_ELEMENT_TO_KEY[cfg.elementId] = key;
